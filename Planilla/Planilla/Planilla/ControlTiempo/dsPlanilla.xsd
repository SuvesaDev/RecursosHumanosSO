		<xs:unique name="Planilla_Deducciones_Constraint1" msdata:ConstraintName="Constraint1" msdata:PrimaryKey="true">
			<xs:selector xpath=".//mstns:Planilla_Deducciones" />
			<xs:field xpath="mstns:Id" />
		</xs:unique>
		<xs:unique name="Planilla_Detalle_Constraint1" msdata:ConstraintName="Constraint1" msdata:PrimaryKey="true">
			<xs:selector xpath=".//mstns:Planilla_Detalle" />
			<xs:field xpath="mstns:Id" />
		</xs:unique>

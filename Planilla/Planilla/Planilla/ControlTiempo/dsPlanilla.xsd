		<xs:unique name="Horas_Ordinarias_Constraint1" msdata:ConstraintName="Constraint1" msdata:PrimaryKey="true">
			<xs:selector xpath=".//mstns:Horas_Ordinarias" />
			<xs:field xpath="mstns:Horas" />
		</xs:unique>
		<xs:keyref name="PlanillaPlanilla_Detalle" refer="Planilla_Constraint1">
			<xs:selector xpath=".//mstns:Planilla_Detalle" />
			<xs:field xpath="mstns:Id_Planilla" />
		</xs:keyref>
		<xs:keyref name="Planilla_DetallePlanilla_Deducciones" refer="Planilla_Detalle_Constraint1">
			<xs:selector xpath=".//mstns:Planilla_Deducciones" />
			<xs:field xpath="mstns:Id_PlanillaDetalle" />
		</xs:keyref>
	</xs:element>
</xs:schema>
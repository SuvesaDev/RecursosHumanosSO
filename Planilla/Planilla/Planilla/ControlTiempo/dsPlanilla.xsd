							<xs:element name="Fecha" type="xs:dateTime" />
							<xs:element name="HoraOrdinaria" type="xs:double" />
							<xs:element name="HoraExtra" type="xs:string" />
							<xs:element name="HoraDoble" type="xs:string" />
							<xs:element name="Id_Proyecto" type="xs:int" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="ParametroRenta">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Id" msdata:ReadOnly="true" msdata:AutoIncrement="true" type="xs:int" />
							<xs:element name="MontoExento" type="xs:double" />
							<xs:element name="MontoDe" type="xs:double" />
							<xs:element name="MontoHasta" type="xs:double" />
							<xs:element name="Porcentaje1" type="xs:double" />
							<xs:element name="MontoEnAdelante" type="xs:double" />

							<xs:element name="Numero" type="xs:long" />
							<xs:element name="Monto" type="xs:double" />
							<xs:element name="Tipo_Pago" type="xs:boolean" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Planilla">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Id_Planilla" msdata:ReadOnly="true" msdata:AutoIncrement="true" type="xs:long" />
							<xs:element name="Fecha" type="xs:dateTime" />
							<xs:element name="FechaInicial" type="xs:dateTime" />
							<xs:element name="FechaFinal" type="xs:dateTime" />
							<xs:element name="CierreMes" type="xs:boolean" />

							<xs:element name="ValorCompra" type="xs:double" />
							<xs:element name="ValorVenta" type="xs:double" />
							<xs:element name="Simbolo" type="xs:string" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Horas_Ordinarias">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Horas" type="xs:int" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
